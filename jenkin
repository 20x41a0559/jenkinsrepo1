pipeline {
    agent any

    stages {
        stage('check validate') {
            steps {
                echo 'i am validitating  the code'
            }
        }
     stage('compilation') {
            steps {
                echo 'i am compiling  the code'
            }
         }
      stage('install') {
            steps {
                echo 'i am installing  the code'
            }
        }
     stage('deploy') {
            steps {
                echo 'i am deploying the code'
            }
        }
    }
}

